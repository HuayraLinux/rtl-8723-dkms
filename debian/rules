#!/usr/bin/make -f

VERSION=$(shell dpkg-parsechangelog |grep ^Version:|cut -d ' ' -f 2)

%:
	dh $@ --with dkms

override_dh_install:
	dh_install Makefile *.c *.h btcoexist rtl* compat*.bz2 usr/src/rtl-8723-dkms-$(VERSION)/

override_dh_dkms:
	dh_dkms -V $(VERSION)

override_dh_auto_build:

override_dh_auto_install:
